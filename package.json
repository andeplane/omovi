{
  "name": "omovi",
  "version": "0.17.0",
  "description": "Online Molecular Visualizer",
  "author": "andeplane",
  "license": "GPLv3",
  "repository": "andeplane/omovi",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "start": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "prettier --check 'src/**/*.{ts,tsx}'",
    "lint:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.0",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.14",
    "buffer": "^5.7.1",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.2.0",
    "parcel": "^2.8.0",
    "prettier": "^3.7.2",
    "process": "^0.11.10",
    "typescript": "^5.9.3",
    "util": "^0.12.5",
    "uuid": "^13.0.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^4.0.14"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/binary-parser": "^1.5.0",
    "@types/three": "^0.181.0",
    "binary-parser": "^1.7.0",
    "easy-peasy": "^5.0.3",
    "kd-tree-javascript": "^1.0.3",
    "n8ao": "^1.7.0",
    "postprocessing": "^6.35.0",
    "stats.js": "^0.17.0",
    "strict-uri-encode": "^1.1.0",
    "three": "^0.181.2"
  }
}
